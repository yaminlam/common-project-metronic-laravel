{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "dedoc/scramble": "^0.12.17",
        "kreait/laravel-firebase": "^6.0",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.27",
        "livewire/livewire": "^3.5",
        "maatwebsite/excel": "^3.1.58",
        "mpdf/mpdf": "^8.2",
        "opcodesio/log-viewer": "^3.10",
        "php-flasher/flasher-laravel": "^2.1",
        "spatie/laravel-activitylog": "^4.9",
        "spatie/laravel-image-optimizer": "^1.8.2",
        "spatie/laravel-medialibrary": "^11.5",
        "spatie/laravel-settings": "^3.3",
        "tightenco/ziggy": "^2.2",
        "yajra/laravel-datatables": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.0",
        "captainhook/captainhook": "^5.25",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/phpinsights": "^2.12",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "spatie/laravel-ignition": "^2.4",
        "squizlabs/php_codesniffer": "^3.9",
        "tightenco/duster": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/CommonHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pint": [
            "./vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
